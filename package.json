{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "hexo clean",
    "deploy": "hexo deploy",
    "dev": "hexo server",
    "push:master": "cd themes/stellar && git push origin main:main && cd ../.. && git push origin master:master && git push gh master:master",
    "fix-images": "node builder/fixImages",
    "pull": "git submodule foreach git pull origin main:main && git pull origin master:master",
    "c": "hexo clean",
    "g": "hexo g",
    "cg": "hexo clean && hexo g",
    "p": "pnpm push:master",
    "d": "hexo clean && hexo g && npm run fix-images && hexo d",
    "pd": "npm run p && npm run d"
  },
  "hexo": {
    "version": "7.1.1"
  },
  "dependencies": {
    "hexo": "^7.1.1",
    "hexo-fs": "^4.1.1",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-index": "^3.0.0",
    "hexo-generator-tag": "^2.0.0",
    "hexo-related-popular-posts": "^5.0.1",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-marked": "^6.2.0",
    "hexo-renderer-stylus": "^3.0.1",
    "hexo-server": "^3.0.0",
    "hexo-util": "^3.2.0"
  },
  "devDependencies": {
    "hexo-deployer-git": "^4.0.0"
  }
}